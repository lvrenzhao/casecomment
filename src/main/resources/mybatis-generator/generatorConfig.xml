<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="sysconfig.properties"/>
    <classPathEntry location="/Users/lvrenzhao/IdeaProjects/casecomment/target/casecomment/WEB-INF/lib/mysql-connector-java-5.1.30.jar"/>
    <context id="my" targetRuntime="MyBatis3Simple">
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="${url}" userId="${username}"
                        password="${password}"/>

        <javaModelGenerator targetPackage="cn.gov.ahcourt.casecomment.bean" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="cn.gov.ahcourt.casecomment.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="cn.gov.ahcourt.casecomment.mapper" targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--<table tableName="" domainObjectName=""></table>-->

    </context>
</generatorConfiguration>